{
  "name": "pro-template",
  "version": "0.0.1",
  "description": "An out-of-box UI solution for enterprise applications",
  "main": "index.js",
  "author": "zhangj",
  "license": "AGPL-3",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && yarn lint:fix && yarn lint:script && git add ."
    }
  },
  "scripts": {
    "start": "rwp --dev",
    "build": "rwp --build",
    "analyzer": "rwp --analyzer",
    "lint:script": "eslint ./src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint ./src/**/*.{js,jsx,ts,tsx} --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "dependencies": {
    "@rwp/core": "~0.28.0-alpha.0",
    "@rwp/react-ui": "~0.30.1-alpha.0",
    "@rwp/render-react": "~0.19.0-alpha.0",
    "mobx-react-lite": "~2.0.7",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-router-dom": "~5.2.0",
    "umi-request": "~1.3.5"
  },
  "devDependencies": {
    "@types/react": "~16.9.43",
    "@types/react-router-dom": "~5.1.5",
    "@umijs/fabric": "~2.2.2",
    "babel-loader": "~8.1.0",
    "husky": "~4.2.5",
    "less-loader": "~6.2.0",
    "prettier": "~2.0.5"
  }
}
